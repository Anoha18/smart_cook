{
    "ignore": [
      "**/*.test.ts",
      "**/*.spec.ts",
      ".git",
      "node_modules",
      "graphQL/models"
    ],
    "restartable": "rs",
    "verbose": true,
    "execMap": {
      "ts": "node --require ts-node/register"
    },
    "watch": [
      "*"
    ],
    "exec": "npm run build_ts && node",
    "ext": "js,json,ts"
}